{
  "name": "KillerRobot",
  "description": "Kills unpinned tabs in a window for increased performace! \n Click to kill tabs, click again to refresh.",
  "version": "1.7",
  "background": {
    "scripts": ["app.js"]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
      "default_title": "KillerRobot",
      "default_icon": "icon.png"
  },
  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "manifest_version": 2
}
